    "express-validator": "^6.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "node-sass": "^4.14.1",
    "prop-types": "^15.6.1",
    "react-flags": "^0.1.13",
    "react-hook-form": "^6.15.4",
    "react-id-swiper": "^4.0.0",
